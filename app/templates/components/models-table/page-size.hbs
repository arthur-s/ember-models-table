  <div class="{{if useNumericPagination 'pure-u-2-3 pure-u-md-2-3' 'pure-u-1 pure-u-md-1-3'}}">
    <div class="pull-right pure-form">

      <select class="changePageSize" {{action 'changePageSize' on='change'}}>
        {{#each pageSizeValues key="@index" as |item|}}
          <option value="{{item}}" selected={{is-equal item selectedValue}}>
            {{item}}
          </option>
        {{/each}}
      </select>

    </div>
  </div> 